{
  "manifest_version": 2,

  "name": "Journey",
  "description": "This extension allows you to read a news feed from yourself one year ago.",
  "version": "0.0.1",
  "chrome_url_overrides" : {
    "newtab": "journey.html"
  },
  "background": 
  {
    "scripts": ["js/journey/update.js", "js/background.js"],
    "persistent" : false
  },
  "permissions": [
    "tabs",
    "https://*.facebook.com/*",
    "http://*.facebook.com/*"
  ],
  "icons" : { 
    "16" : "img/logo-16.png", 
    "48" : "img/logo-48.png",
    "128" : "img/logo-128.png"
  }
}